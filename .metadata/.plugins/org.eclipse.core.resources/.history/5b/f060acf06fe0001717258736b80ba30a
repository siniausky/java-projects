package Exercises;

public class PrintATable {

	public static void main(String[] args) {
		int a=1, b=2;
		System.out.println("a\tb\tpow(a, b)");
		System.out.println(a + "\t" + b +"\t" + Math.pow(a++, b++));
		System.out.println("a\tb\tpow(a, b)");
		System.out.println("a\tb\tpow(a, b)");
		System.out.println("a\tb\tpow(a, b)");
		System.out.println("a\tb\tpow(a, b)");

	}

}
